  "main": "n/a",
  "scripts": {
    "dev:ip": "npm-run-all --parallel gatsby:ip start:lambda",
    "dev:local": "npm-run-all --parallel gatsby:local start:lambda",
    "test:ip": "start-server-and-test dev:ip http://192.168.1.14:8000 cy:open",
    "test:local": "start-server-and-test dev:local http://localhost:8000 cy:open",
    "clean": "rm -rf public && rm -rf .cache",
    "clean:cache": "rm -rf public && rm -rf .cache",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "gatsby:ip": "gatsby develop -H 192.168.1.14",
    "gatsby:local": "gatsby develop",
    "test": "jest --watch",
    "cy:open": "cypress open",
    "netlify": "npm-run-all --parallel build build:lambda",
    "start:lambda": "netlify-lambda serve functions -c webpack.functions.js",
    "build:lambda": "netlify-lambda build functions -c webpack.functions.js",
    "sprite": "gulp build:svg",
    "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check"
  },
  "browserslist": [
    ">0.20%",
    "not dead",
    "ie 11"
  ],
