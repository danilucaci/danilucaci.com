{
  "name": "danilucaci.com",
  "description": "danilucaci.com Personal Blog",
  "private": true,
  "author": "Dani Lucaci <hello@danilucaci.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "personal blog"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danilucaci/danilucaci"
  },
  "main": "n/a",
  "scripts": {
    "dev:ip": "gatsby develop -H 192.168.1.13",
    "dev:local": "gatsby develop",
    "serve:ip": "npm run build && gatsby serve -H 192.168.1.13",
    "serve:local": "npm run build && gatsby serve",
    "test:jest": "jest --watch",
    "test:ip:dev": "npm-run-all --parallel dev:ip cy:open:dev",
    "test:ip:prod": "npm-run-all --parallel dev:ip cy:open:prod",
    "test:local:dev": "npm-run-all --parallel dev:local cy:open:dev",
    "test:local:prod": "npm-run-all --parallel dev:local cy:open:prod",
    "cy:open:dev": "cypress open --env configFile=dev",
    "cy:open:prod": "cypress open --env configFile=prod",
    "clean": "rm -rf public && rm -rf .cache",
    "clean:cache": "rm -rf public && rm -rf .cache",
    "serve": "gatsby serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "netlify": "npm run build",
    "sprite": "gulp build:svg"
  },
  "browserslist": [
    ">0.20%",
    "not dead",
    "ie 11"
  ],
