{
  "name": "api",
  "description": "danilucaci.com API Endpoints",
  "private": true,
  "version": "1.0.0",
  "author": "Dani Lucaci <hello@danilucaci.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "personal blog",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danilucaci/danilucaci"
  },
  "main": "./src/index.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/index.js",
    "start": "node src/index.js",
    "test:watch": "jest --watchAll",
    "test:related": "jest --bail --findRelatedTests",
    "test:ci:all": "jest --bail",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "yarn run lint:js -- --fix",
    "lint:format": "prettier --write .",
    "lint:format:check": "prettier --check .",
    "pre:commit": "lint-staged",
    "pre:push": "yarn run lint:js && yarn run lint:format:check && yarn run test:ci:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "loglevel": "^1.7.1",
    "mongoose": "^5.11.14",
    "morgan": "^1.10.0",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^26.6.3",
    "jest-environment-node": "^26.6.2",
    "mongodb-memory-server": "^6.9.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3"
  }
}
