{
  "name": "web",
  "description": "danilucaci.com Personal Blog",
  "private": true,
  "version": "1.0.0",
  "author": "Dani Lucaci <hello@danilucaci.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "personal blog"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danilucaci/danilucaci"
  },
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop -H 192.168.0.27",
    "dev:local": "gatsby develop",
    "serve:ip": "yarn run build && gatsby serve -H 192.168.0.27",
    "serve:local": "yarn run build && gatsby serve",
    "test:watch": "jest --watchAll",
    "test:related": "jest --bail --findRelatedTests --env=jsdom",
    "test:ci:all": "cross-env CI=true jest --bail --env=jsdom",
    "cy:run:dev": "cypress run --env configFile=dev",
    "cy:run:prod": "cypress run --env configFile=prod",
    "cy:dev": "npm-run-all --parallel dev cy:open:dev",
    "cy:prod": "npm-run-all --parallel dev cy:open:prod",
    "cy:local:dev": "npm-run-all --parallel dev:local cy:open:dev",
    "cy:local:prod": "npm-run-all --parallel dev:local cy:open:prod",
    "cy:open:dev": "cypress open --env configFile=dev",
    "cy:open:prod": "cypress open --env configFile=prod",
    "clean": "rm -rf public && rm -rf .cache",
    "serve": "gatsby serve",
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build",
    "netlify:build": "yarn run build",
    "sprite": "gulp build:svg",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "yarn run lint:js -- --fix",
    "lint:format": "prettier --write .",
    "lint:format:check": "prettier --check .",
    "pre:commit": "lint-staged",
    "pre:push": "yarn run lint:js && yarn run lint:format:check && yarn run test:ci:all"
  },
  "browserslist": [
    ">0.20%",
    "not dead",
    "ie 11"
  ],
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@reach/menu-button": "^0.13.0",
    "@sentry/browser": "^6.2.0",
    "@sentry/node": "^6.2.0",
    "@sentry/webpack-plugin": "^1.14.1",
    "axios": "^0.21.1",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^3.9.0",
    "cypress": "6.5.0",
    "date-fns": "^2.17.0",
    "disqus-react": "^1.0.11",
    "eslint-plugin-prettier": "^3.4.0",
    "event-source-polyfill": "^1.0.22",
    "firebase": "^8.2.9",
    "firebase-tools": "^9.4.0",
    "fontfaceobserver": "^2.1.0",
    "formik": "^2.2.6",
    "gatsby": "^2.32.4",
    "gatsby-image": "^2.11.0",
    "gatsby-plugin-catch-links": "^2.10.0",
    "gatsby-plugin-feed": "^2.13.0",
    "gatsby-plugin-lodash": "^3.10.0",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^2.12.0",
    "gatsby-plugin-mdx": "^1.10.0",
    "gatsby-plugin-offline": "^3.10.1",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-react-helmet": "^3.10.0",
    "gatsby-plugin-sharp": "^2.14.1",
    "gatsby-plugin-sitemap": "^2.12.0",
    "gatsby-plugin-styled-components": "^3.10.0",
    "gatsby-remark-autolink-headers": "^2.11.0",
    "gatsby-remark-copy-linked-files": "^2.10.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-images": "^3.11.0",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-remark-responsive-iframe": "^2.11.0",
    "gatsby-source-filesystem": "^2.11.0",
    "gatsby-transformer-remark": "^2.16.0",
    "gatsby-transformer-sharp": "^2.12.0",
    "intl": "^1.2.5",
    "js-cookie": "^2.2.1",
    "mini-svg-data-uri": "^1.2.3",
    "prismjs": "^1.23.0",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-intersection-observer": "^8.31.0",
    "react-intl": "^5.12.3",
    "react-router-dom": "^5.2.0",
    "sentry": "^0.1.2",
    "sharp": "^0.27.1",
    "styled-components": "^5.2.1",
