{
  "name": "web",
  "description": "danilucaci.com Personal Blog",
  "private": true,
  "version": "1.0.0",
  "author": "Dani Lucaci <hello@danilucaci.com>",
  "license": "MIT",
  "keywords": [
    "gatsby",
    "portfolio",
    "personal blog"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/danilucaci/danilucaci"
  },
  "main": "n/a",
  "scripts": {
    "dev": "gatsby develop -H 192.168.1.15",
    "dev:local": "gatsby develop",
    "serve:ip": "yarn run build && gatsby serve -H 192.168.1.15",
    "serve:local": "yarn run build && gatsby serve",
    "test:watch": "jest --watchAll",
    "test:related": "jest --bail --findRelatedTests --passWithNoTests --env=jsdom",
    "test:ci:all": "cross-env CI=true jest --bail --env=jsdom",
    "cy:run:dev": "cypress run --env configFile=dev",
    "cy:run:prod": "cypress run --env configFile=prod",
    "cy:dev": "npm-run-all --parallel dev cy:open:dev",
    "cy:prod": "npm-run-all --parallel dev cy:open:prod",
    "cy:local:dev": "npm-run-all --parallel dev:local cy:open:dev",
    "cy:local:prod": "npm-run-all --parallel dev:local cy:open:prod",
    "cy:open:dev": "cypress open --env configFile=dev",
    "cy:open:prod": "cypress open --env configFile=prod",
    "clean": "rm -rf public && rm -rf .cache",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "netlify:build": "yarn run build",
    "sprite": "gulp build:svg",
    "lint:js": "eslint . --ext .js",
    "lint:js:fix": "yarn run lint:js -- --fix",
    "lint:format": "prettier --write .",
    "lint:format:check": "prettier --check .",
    "pre:commit": "lint-staged",
    "pre:push": "yarn run lint:js && yarn run lint:format:check && yarn run test:ci:all"
  },
  "browserslist": [
    ">0.20%",
    "not dead",
    "ie 11"
  ],
  "dependencies": {
    "@mdx-js/mdx": "1.6.22",
